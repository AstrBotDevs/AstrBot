    "@tiptap/vue-3": "2.14.0",
    "apexcharts": "4.7.0",
    "axios": "^1.10.0",
    "axios-mock-adapter": "^2.1.0",
    "chance": "1.1.13",
    "d3": "^7.9.0",
    "date-fns": "4.1.0",
    "highlight.js": "^11.11.1",
    "js-md5": "^0.8.3",
    "lodash": "4.17.21",
    "marked": "^15.0.12",
    "pinia": "3.0.3",
    "remixicon": "4.6.0",
    "vee-validate": "4.15.1",
    "vite-plugin-vuetify": "2.1.1",
    "vue": "3.5.17",
    "vue-router": "4.5.1",
